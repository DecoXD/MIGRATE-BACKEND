{
    "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "noImplicitAny": false,
    "esModuleInterop": true,
    "removeComments": true,
    "skipLibCheck": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "sourceMap": true,
    "paths": {
      "@utils/*":["./src/utilities/*"],
      "@interfaces/*":["./src/interfaces/*"],
      "@services/*":["./src/services/*"]

    }
    
    
    },
      "include": ["src/**/*"],
      
      "exclude": ["**/*.spec.ts","node_modules"]
  }